{ 
    "extends": "../../.eslintrc", 
    "rules": {
        "@nrwl/nx/enforce-module-boundaries": "off",
        "dot-notation": "off",
        "no-unused-vars": "off",
        "no-use-before-define": "off"
    }, 
    "env": {
        "browser": true,
        "amd": true,
        "node": true,
        "es6": true
    },
    "ignorePatterns": [
        "node_modules/**",
        "dist/**",
        "coverage/**",
        "tmp/**"
    ] 
}